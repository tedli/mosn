{
    "admin": {
        "address": {
            "socket_address": {
                "address": "0.0.0.0",
                "port_value": 34901
            }
        }
    },
    "metrics": {
        "shm_zone": "metrics",
        "shm_size": "10MB",
        "flush_interval": "500ms",
        "sinks": [
            {
                "type": "prometheus",
                "config": {
                    "port": 15090
                }
            }
        ]
    },
    "static_resources": {
        "clusters": [
            {
                "name": "xds-grpc",
                "type": "STRICT_DNS",
                "respect_dns_ttl": true,
                "dns_lookup_family": "V4_ONLY",
                "connect_timeout": "1s",
                "lb_policy": "ROUND_ROBIN",
                "load_assignment": {
                    "cluster_name": "xds-grpc",
                    "endpoints": [
                        {
                            "lb_endpoints": [
                                {
                                    "endpoint": {
                                        "address": {
                                            "socket_address": {
                                                "address": "127.0.0.1",
                                                "port_value": 15010
                                            }
                                        }
                                    }
                                }
                            ]
                        }
                    ]
                }
            }
        ]
    },
    "dynamic_resources": {
        "lds_config": {
            "ads": {}
        },
        "cds_config": {
            "ads": {}
        },
        "ads_config": {
            "api_type": "GRPC",
            "grpc_services": [
                {
                    "envoy_grpc": {
                        "cluster_name": "xds-grpc"
                    }
                }
            ],
            "refresh_delay": {
                "seconds": 300
            }
        }
    }
}